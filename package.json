{
    "name": "bike-rental",
    "private": true,
    "scripts": {
        "dev:frontend": "vite --port 8080",
        "dev:backend": "dotnet run --project ./Starter",
        "open-browser": "start http://localhost:5001",
        "dev": "npm run open-browser & concurrently --kill-others \"npm run dev:frontend\" \"npm run dev:backend\"",
        "build:frontend": "vue-tsc && vite build",
        "build:backend": "(cd ./Starter && dotnet build)",
        "build": "npm run build:frontend && npm run build:backend",
        "preview": "vite preview"
    },
    "dependencies": {
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.1.0",
        "concurrently": "^7.6.0",
        "typescript": "^4.9.3",
        "vite": "^4.2.0",
        "vue-tsc": "^1.2.0"
    }
}
